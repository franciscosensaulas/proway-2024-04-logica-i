algoritmo "Exemplo procedimento 01"
var
numero: inteiro

procedimento solicitarNumero()
inicio
escreva("Digite um número: ")
leia(numero)
fimprocedimento

procedimento apresentarNumero()
inicio
limpatela
escreval("Número: ", numero)
fimprocedimento

inicio
solicitarNumero()
apresentarNumero()
fimalgoritmo
